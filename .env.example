# Realtime Transit Analytics - example environment configuration
# Copy this file to .env and adjust values as needed

# Kafka
KAFKA_BROKER=kafka:9092

# Postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=transit

# Redis
REDIS_URL=redis://redis:6379/0

# TTC realtime
GTFS_VEHICLE_URL=https://bustime.ttc.ca/gtfsrt/vehicles
GTFS_TRIPS_URL=https://bustime.ttc.ca/gtfsrt/trips
KAFKA_TOPIC_VP=transit.ttc.vehicle_positions
KAFKA_TOPIC_TU=transit.ttc.trip_updates
